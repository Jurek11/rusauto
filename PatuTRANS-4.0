		(IIf(<VATadd> = -1, ((SELECT TotalTTC 
						FROM tblCarPATU 
						WHERE ID = <VoitureID>) + <CamionPrice> + <AddTotal>), ((SELECT Total 
						FROM tblCarPATU 
						WHERE ID = <VoitureID>) + <CamionPrice> + <AddTotal>))), 
		(IIf(<VATadd> = -1, ((SELECT TotalTTC 
						FROM tblCarPATU 
